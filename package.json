    "@sencha/ext-react-transition": "~6.6.3",
    "miguelpalacio-atg-react-demo": "0.0.10",
    "miguelpalacio-countdown-timer": "1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-slick": "^0.24.0",
    "slick-carousel": "^1.8.1"
  },
